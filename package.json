{
   "name": "config-eslint",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "tsc && vite build",
      "commitlint": "commitlint --edit",
      "lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
