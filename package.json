      "@mui/styled-engine-sc": "^6.0.0-alpha.4",
      "@reduxjs/toolkit": "^1.9.5",
      "@tanstack/react-query": "^4.36.1",
      "@tanstack/react-table": "^8.10.1",
      "@types/axios": "^0.14.0",
      "@types/nprogress": "^0.2.0",
      "axios": "^1.6.2",
      "combine-reducers": "^1.0.0",
